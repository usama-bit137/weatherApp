{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": "off", 
      "import/prefer-default-export": "off", 
      "no-throw-literal": "off", 
      "quotes": [
        "error", 
        "double"
      ]
    }
}
